<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.student.services.model.Student" />
	<class name="com.student.services.model.impl.StudentImpl" table="student">
		<id name="studentId" type="int">
			<generator class="increment" />
		</id>
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="lastname" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="std" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="address" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="phone" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="gender" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>